<% if (typeof(name) != 'undefined' && typeof(email) != 'undefined' ) { %>
<div class="col-xs-12 col-sm-4 col-sm-offset-4">
  <div class="form-group">
    <label for="exampleInputEmail1">name</label>
    <input type="text"value="<%=name%>" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="my email">

  </div>
  <div class="form-group">
    <label for="exampleInputPassword1">email</label>
    <input type="text" value="<%=email%>"class="form-control" id="exampleInputPassword1" placeholder="my name ">
  </div>
</div>
<%}%>
